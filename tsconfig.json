{
  "compilerOptions": {
    "target": "es2020",
    "module": "nodenext",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "declaration": true,
    "noEmit": false,
    "rewriteRelativeImportExtensions": true
  },
  "include": [
    "server.ts",
    "pages"
  ]
}
